<UserControl xmlns="https://github.com/avaloniaui"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  xmlns:vm="using:Avln_Sample_Template.ViewModels"
        xmlns:vc="using:Avln_Sample_Template.ValueConverter"
  mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Avln_Sample_Template.Views.TemplateView"
        x:DataType="vm:TemplateViewModel">

    <UserControl.Resources>
        <vc:DateTimeValueConverter x:Key="DateTimeValueConverter"/>
    </UserControl.Resources>

    <Design.DataContext>
  <vm:TemplateViewModel/>
    </Design.DataContext>

 <StackPanel>
    <Label Content="Template-View:"/>
      <Grid>
   <TextBlock Text="{Binding Now, ConverterParameter='yyyy-MM-dd HH:mm:ss', Converter={StaticResource DateTimeValueConverter}}"/>
   </Grid>
    </StackPanel>
</UserControl>
