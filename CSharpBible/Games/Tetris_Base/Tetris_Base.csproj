<Project>
	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<BaseIntermediateOutputPath>..\..\..\obj.net\$(MSBuildProjectName)\</BaseIntermediateOutputPath>
		<IntermediateOutputPath>..\..\..\obj.net\$(MSBuildProjectName)\</IntermediateOutputPath>
		<BaseOutputPath>..\..\..\bin\$(MSBuildProjectName)</BaseOutputPath>
		<OutputType>Exe</OutputType>
	</PropertyGroup>
	<Import Sdk="Microsoft.NET.Sdk" Project="Sdk.props" />
	<Import Sdk="Microsoft.NET.Sdk" Project="Sdk.targets" />
	<PropertyGroup>
		<ImplicitUsings>disable</ImplicitUsings>
		<Nullable>enable</Nullable>

		<StartupObject>Tetris_Base.Program</StartupObject>
		<Deterministic>false</Deterministic>
		<GenerateAssemblyFileVersionAttribute>false</GenerateAssemblyFileVersionAttribute>
		<GenerateAssemblyInformationalVersionAttribute>false</GenerateAssemblyInformationalVersionAttribute>
		<AssemblyVersion>1.0.*</AssemblyVersion>
		<Company>JC-Soft</Company>
		<Authors>Joe Care</Authors>
		<Copyright>Copyright Â© JC-Soft 2022</Copyright>
		<Product>Tetris 1.0 Base</Product>
	</PropertyGroup>
	<ItemGroup>
		<Compile Remove="bin\**" />
		<EmbeddedResource Remove="bin\**" />
		<None Remove="bin\**" />
		<Compile Remove="obj\**" />
		<EmbeddedResource Remove="obj\**" />
		<None Remove="obj\**" />
	</ItemGroup>
	<ItemGroup>
	  <ProjectReference Include="..\..\ConsoleDisplay\ConsoleDisplay_net.csproj" />
	</ItemGroup>
	
</Project>
