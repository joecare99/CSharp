<Project>
	<Import Project="..\Games_net.props" />
	<PropertyGroup>
		<TargetFrameworks>net462;net472;net48;net481;net6.0;net7.0;net8.0;net9.0</TargetFrameworks>
		<OutputType>Library</OutputType>
	</PropertyGroup>
	<Import Sdk="Microsoft.NET.Sdk" Project="Sdk.props" />
	<Import Sdk="Microsoft.NET.Sdk" Project="Sdk.targets" />
	<ItemGroup>
	  <Compile Remove="View\**" />
	  <EmbeddedResource Remove="View\**" />
	  <None Remove="View\**" />
	</ItemGroup>
	<ItemGroup>
	  <Compile Remove="Program.cs" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="CommunityToolkit.Mvvm" Version="8.4.0" />
		<PackageReference Condition="$(TargetFramework.StartsWith(net462))" Include="System.ValueTuple" Version="4.5.0" />
	</ItemGroup>
	<ItemGroup>
    <ProjectReference Condition="!$(TargetFramework.StartsWith(net4))"  Include="..\Game_Base\Game_Base_net.csproj"  />
    <ProjectReference Condition="$(TargetFramework.StartsWith(net4))"  Include="..\Game_Base\Game_Base.csproj"  />
  </ItemGroup>

	<ItemGroup>
		<Compile Update="Ressource\Resource1.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>Resource1.resx</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Update="Ressource\Resource1.resx">
			<Generator>ResXFileCodeGenerator</Generator>
			<LastGenOutput>Resource1.Designer.cs</LastGenOutput>
		</EmbeddedResource>
	</ItemGroup>
</Project>
