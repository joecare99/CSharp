<Project>

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
	  <BaseOutputPath>..\..\..\bin\$(MSBuildProjectName)\</BaseOutputPath>
	  <BaseIntermediateOutputPath>..\..\..\obj.net\$(MSBuildProjectName)\</BaseIntermediateOutputPath>
	  <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <Authors>Joe Care</Authors>
    <Company>JC-Soft</Company>
    <Copyright>(c) by Joe Care 2022</Copyright>
    <Description>Darstellungselemente mit Converter automatisch erzeugen.</Description>
  </PropertyGroup>
	<Import Sdk="Microsoft.NET.Sdk" Project="Sdk.props" />
	<Import Sdk="Microsoft.NET.Sdk" Project="Sdk.targets" />
	<ItemGroup>
	  <None Remove="Resources\WernerTiles.png" />
	  <None Remove="View\WernerTiles.png" />
	</ItemGroup>
	<ItemGroup>
	  <ProjectReference Include="..\..\Games\Werner_Flaschbier\Werner_Flaschbier_Base.csproj" />
	  <ProjectReference Include="..\..\Libraries\MVVM_BaseLib\MVVM_BaseLib_net.csproj" />
	</ItemGroup>
	<ItemGroup>
	  <Resource Include="Resources\WernerTiles.png">
	    <CopyToOutputDirectory>Never</CopyToOutputDirectory>
	  </Resource>
	  <Resource Include="View\WernerTiles.png" />
	</ItemGroup>
	<ItemGroup>
	  <Compile Update="Properties\Images.Designer.cs">
	    <DesignTime>True</DesignTime>
	    <AutoGen>True</AutoGen>
	    <DependentUpon>Images.resx</DependentUpon>
	  </Compile>
	</ItemGroup>
	<ItemGroup>
	  <EmbeddedResource Update="Properties\Images.resx">
	    <Generator>ResXFileCodeGenerator</Generator>
	    <LastGenOutput>Images.Designer.cs</LastGenOutput>
	  </EmbeddedResource>
	</ItemGroup>

</Project>
