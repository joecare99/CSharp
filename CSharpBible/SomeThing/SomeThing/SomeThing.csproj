<Project>
	<Import Project="..\Solution.props"/>
	<PropertyGroup>
		<TargetFrameworks>net462;net472;net48;net481;net6.0;net7.0;net8.0</TargetFrameworks>
		<OutputType>Exe</OutputType>
	</PropertyGroup>
	<Import Sdk="Microsoft.NET.Sdk" Project="Sdk.props" />
	<Import Sdk="Microsoft.NET.Sdk" Project="Sdk.targets" />
	<PropertyGroup>
		<ImplicitUsings>disable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Condition="'!$(TargetFramework.StartsWith(net4))'" Include="$(UpDir)\Libararies\ConsoleDisplay\ConsoleDisplay_net.csproj" />
		<ProjectReference Condition="'$(TargetFramework.StartsWith(net4))'" Include="$(UpDir)\Libraries\ConsoleDisplay\ConsoleDisplay.csproj" />
	</ItemGroup>
</Project>
