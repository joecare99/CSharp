<Project>
	<Import Project="..\Libraries.props" />
	<PropertyGroup>
		<OutputType>Library</OutputType>
    <TargetFrameworks>net6.0;net7.0;net8.0;net9.0;net481;net48;net472;net462</TargetFrameworks>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>
	<Import Sdk="Microsoft.NET.Sdk" Project="Sdk.props" />
	<Import Sdk="Microsoft.NET.Sdk" Project="Sdk.targets" />
	<PropertyGroup>
	</PropertyGroup>
	<ItemGroup>
		<Compile Remove="obj\**" />
		<EmbeddedResource Remove="obj\**" />
		<None Remove="obj\**" />
	</ItemGroup>
	<ItemGroup>
	  <Compile Remove="View\Interfaces\IConsole.cs" />
	  <Compile Remove="View\MyConsoleBase.cs" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Condition="'$(TargetFramework)'=='net462'" Include="System.ValueTuple" Version="4.6.1" />
		<PackageReference Condition="$(TargetFramework.StartsWith(net4))" Include="IndexRange" Version="1.1.0" />
	</ItemGroup>
	<ItemGroup>
	  <ProjectReference Include="..\BaseLib\BaseLib.csproj" />
	</ItemGroup>
</Project>
