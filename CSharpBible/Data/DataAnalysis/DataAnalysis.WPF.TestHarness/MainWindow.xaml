<Window x:Class="DataAnalysis.WPF.TestHarness.MainWindow"
 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
 xmlns:views="clr-namespace:DataAnalysis.WPF.Views;assembly=DataAnalysis.WPF"
 xmlns:viewModels="clr-namespace:DataAnalysis.WPF.ViewModels;assembly=DataAnalysis.WPF"
 Title="Test Harness" Height="800" Width="1200">
 <Grid>
 <Grid.RowDefinitions>
 <RowDefinition Height="Auto"/>
 <RowDefinition Height="*"/>
 </Grid.RowDefinitions>
 <StackPanel Orientation="Horizontal" Margin="8">
 <Button Content="Load Sample Series" Click="OnLoadSeries" Margin="0,0,8,0"/>
 <Button Content="Load Sample Matrix" Click="OnLoadMatrix"/>
 </StackPanel>
 <ContentControl Grid.Row="1" x:Name="Host"/>
 </Grid>
</Window>
