<Project>
	<Import Project="..\Calc_net.props" />
	<PropertyGroup>
		<OutputType>Library</OutputType>
		<TargetFrameworks>net6.0;net7.0;net4.8.1;net4.8;net4.7.2;net4.6.2</TargetFrameworks>
	</PropertyGroup>
	<Import Sdk="Microsoft.NET.Sdk" Project="Sdk.props" />
	<Import Sdk="Microsoft.NET.Sdk" Project="Sdk.targets" />
	<PropertyGroup>
		<RootNamespace>$(MSBuildProjectName.Replace(" ", "_").Replace("_net",""))</RootNamespace>
	</PropertyGroup>
	<ItemGroup>
		<Compile Remove="obj\**" />
		<EmbeddedResource Remove="obj\**" />
		<None Remove="obj\**" />
	</ItemGroup>
	<ItemGroup>
		<Compile Remove="Properties\AssemblyInfo.cs" />
	</ItemGroup>
	<ItemGroup>
		<None Remove="*.bak" />
	</ItemGroup>
	<ItemGroup>
		<Folder Include="Properties\" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\..\Libraries\BaseLib\BaseLib_net.csproj" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Condition="'$(TargetFrameWork)'=='net4.6.2'" Include="System.ValueTuple" Version="4.5.0" />
		<PackageReference Condition="'$(TargetFrameWork)'=='net462-windows'" Include="System.ValueTuple" Version="4.5.0" />
	</ItemGroup>
</Project>