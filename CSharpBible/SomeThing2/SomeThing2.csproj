<Project>
	<Import Project="..\Solution.props"/>
	<PropertyGroup>
		<TargetFrameworks>net462;net472;net48;net481;net6.0;net7.0</TargetFrameworks>
		<OutputType>Exe</OutputType>
	</PropertyGroup>
	<Import Sdk="Microsoft.NET.Sdk" Project="Sdk.props" />
	<Import Sdk="Microsoft.NET.Sdk" Project="Sdk.targets" />
	<PropertyGroup>
		<ImplicitUsings>disable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Condition="'$(TargetFramework)'=='net6.0'" Include="..\ConsoleDisplay\ConsoleDisplay_net.csproj" />
		<ProjectReference Condition="'$(TargetFramework)'=='net7.0'" Include="..\ConsoleDisplay\ConsoleDisplay_net.csproj" />
		<ProjectReference Condition="'$(TargetFramework)'=='net462'" Include="..\ConsoleDisplay\ConsoleDisplay.csproj" />
		<ProjectReference Condition="'$(TargetFramework)'=='net472'" Include="..\ConsoleDisplay\ConsoleDisplay.csproj" />
		<ProjectReference Condition="'$(TargetFramework)'=='net48'" Include="..\ConsoleDisplay\ConsoleDisplay.csproj" />
		<ProjectReference Condition="'$(TargetFramework)'=='net481'" Include="..\ConsoleDisplay\ConsoleDisplay.csproj" />
	</ItemGroup>
</Project>
