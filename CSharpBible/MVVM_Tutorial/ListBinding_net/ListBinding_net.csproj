<Project>
	<Import Project="..\MVVM_Tutorial_net.props"/>

	<PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <BaseOutputPath>..\..\..\bin\$(MSBuildProjectName)\</BaseOutputPath>
    <BaseIntermediateOutputPath>..\..\..\obj.net\$(MSBuildProjectName)\</BaseIntermediateOutputPath>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>
  <Import Sdk="Microsoft.NET.Sdk" Project="Sdk.props" />
  <Import Sdk="Microsoft.NET.Sdk" Project="Sdk.targets" />
  <ItemGroup>
    <Compile Remove="obj\**" />
    <EmbeddedResource Remove="obj\**" />
    <None Remove="obj\**" />
    <Page Remove="obj\**" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Model\" />
	<Folder Include="ViewModel\" />
	<Compile Include="..\ListBinding\Model\Person.cs" Link="Model\Person.cs" />
	<Compile Include="..\ListBinding\ViewModel\PersonViewViewModel.cs" Link="ViewModel\PersonViewViewModel.cs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Libraries\MVVM_BaseLib\MVVM_BaseLib_net.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Page Update="View\PersonView.xaml">
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>
