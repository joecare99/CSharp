<Project>
	<Import Project="..\MVVM_Tutorial_net.props"/>

	<PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <BaseOutputPath>..\..\..\bin\$(MSBuildProjectName)\</BaseOutputPath>
    <BaseIntermediateOutputPath>..\..\..\obj.net\$(MSBuildProjectName)\</BaseIntermediateOutputPath>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>
  <Import Sdk="Microsoft.NET.Sdk" Project="Sdk.props" />
  <Import Sdk="Microsoft.NET.Sdk" Project="Sdk.targets" />
  <ItemGroup>
    <Compile Remove="obj\**" />
    <Compile Remove="ValueConverter\**" />
    <EmbeddedResource Remove="obj\**" />
    <EmbeddedResource Remove="ValueConverter\**" />
    <None Remove="obj\**" />
    <None Remove="ValueConverter\**" />
    <Page Remove="obj\**" />
    <Page Remove="ValueConverter\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="*.bak" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Libraries\MVVM_BaseLib\MVVM_BaseLib_net.csproj" />
  </ItemGroup>

</Project>
