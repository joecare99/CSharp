<Window x:Class="GenFreeBrowser.ViewHost.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:views="clr-namespace:GenFreeBrowser.Views;assembly=GenFreeBrowser"
        xmlns:host="clr-namespace:GenFreeBrowser.ViewHost"
        xmlns:vm="clr-namespace:GenFreeBrowser.ViewHost.ViewModels"
        Title="View Host" Height="600" Width="1200">
    <Grid>
        <TabControl>
            <TabItem Header="Personen Liste">
                <views:FraPersonenList />
            </TabItem>
            <TabItem Header="Kern Familie">
                <views:KernFamilieView />
            </TabItem>
            <TabItem Header="FamilyView (Dummy)">
                <views:FamilyView>
                    <views:FamilyView.DataContext>
                        <vm:FamilyViewDummyVM />
                    </views:FamilyView.DataContext>
                </views:FamilyView>
            </TabItem>
        </TabControl>
    </Grid>
</Window>